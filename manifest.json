{
  "manifest_version": 2,
  "name": "InstaSuggestedBlocker",
  "version": "1.0",
  "description": "Blocks suggested posts from the Insta website",
  "icons": {
    "96": "icons/blocker-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["blocker.js"]
    }
  ]
}